<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="WinnersView" table="winnersview">
        <id name="award_id" type="java.lang.String" column="award_id">
            <generator class="assigned"></generator>  <!-- automatyczna generacja klucza glownego -->
        </id>
        <property name="award_year" column="award_year"></property>
        <property name="nameAndLastName" column="nameAndLastName"></property>
        <property name="actorAge" column="actorAge"></property>
        <property name="movie" column="movie"></property>
        <property name="gender" column="gender"></property>

    </class>

</hibernate-mapping>

        <!-- mapping types
        https://www.tutorialspoint.com/hibernate/hibernate_mapping_types.htm
        -->
